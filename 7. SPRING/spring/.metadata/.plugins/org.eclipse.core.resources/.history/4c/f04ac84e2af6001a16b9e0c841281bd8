package com.test.spring.di.two;

import java.util.Scanner;

public class Util {
	
	public String getName() {
		
		//Ex02 객체 -> (의존) -> Util 객체 -> (의존) -> Scanner 객체
		
		//Util의 의존 객체
		Scanner scan = new Scanner(System.in);
		
		System.out.print("이름 : ");
		
		String name = scan.nextLine();
		
		return name;
		
	}

}
